		"tweet_button_display": {
			"type": "string",
			"default": "full"
		},
		"tweet_styles_disabled": {
			"type": "boolean",
			"default": false
		},
		"anchor": {
			"type": "string",
			"default": ""
		},
		"anchor_prefix": {
			"type": "string",
			"default": "qdlx"
		},
		"has_anchor": {
			"type": "boolean",
			"default": true
