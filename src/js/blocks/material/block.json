			"template": "purple-bliss",
			"enable_contextual_menu": true,
			"enable_copy_text": true,
			"enable_copy_tweet": true,
			"enable_copy_link": true,
			"enable_tweet_this": true,
			"maximum_width": 800,
			"maximum_width_unit": "px"
		}
	},
	"supports": {
