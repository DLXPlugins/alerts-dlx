			"type": "boolean",
			"default": true
		},
		"enable_contextual_menu": {
			"type": "boolean",
			"default": true
		},
		"enable_copy_text": {
			"type": "boolean",
			"default": true
		},
		"enable_copy_tweet": {
			"type": "boolean",
			"default": true
		},
		"enable_copy_link": {
			"type": "boolean",
			"default": true
		},
		"enable_tweet_this": {
			"type": "boolean",
			"default": true
		},
		"preview": {
			"type": "boolean",
			"default": false
		},
		"blockOnly": {
			"type": "boolean",
			"default": false
		},
		"defaultsApplied": {
			"type": "boolean",
			"default": false
		},
		"transforming": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {
